var <%= controller_class_name %> = function($scope, $http, wire) {
  $scope.<%= model_list_name %> = wire;
  $scope.<%= model_parameter_name %> = wire[0] || {};

  $scope.show = function(user) {
     console.log("SHOW USER");
     console.log(user);
  };
    
  $scope.edit = function(user) {
     console.log("EDIT USER");
     console.log(user);
  };
       
  $scope.destroy = function(user) {
      console.log("DESTROY USER");
      console.log(user);
  };
}
